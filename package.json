{
    "name": "atheera",
    "version": "1.0.0",
    "main": "app.js",
    "scripts": {
        "start": "npm run build && node app.js",
        "dev": "nodemon -e js,json --ignore public/ app.js",
        "dev:css": "npx tailwindcss -i ./src/input.css -o ./public/stylesheets/output.css --watch",
        "build:css": "npx tailwindcss -i ./src/input.css -o ./public/stylesheets/output.css --minify",
        "build:react": "webpack --mode production",
        "build": "npm run build:css && npm run build:react",
        "dev:react": "webpack serve --mode development",
        "dev:full": "concurrently \"npm run dev\" \"npm run dev:css\" \"npm run dev:react\"",
        "lint": "eslint .",
        "test": "jest"
    },
    "keywords": [
        "ecommerce",
        "clothing",
        "fashion",
        "express",
        "mongodb"
    ],
    "author": "Yash Varma",
    "license": "ISC",
    "description": "Atheera - A fashion e-commerce platform",
    "dependencies": {
        "@babel/core": "^7.23.7",
        "@babel/preset-env": "^7.23.8",
        "@babel/preset-react": "^7.23.3",
        "@tailwindcss/forms": "^0.5.9",
        "autoprefixer": "^10.4.20",
        "bcryptjs": "^3.0.2",
        "cloudinary": "^2.6.1",
        "compression": "^1.7.4",
        "connect-flash": "^0.1.1",
        "connect-mongo": "^5.1.0",
        "cookie-parser": "^1.4.7",
        "debug": "^4.4.0",
        "dotenv": "^16.4.5",
        "ejs": "^3.1.10",
        "express": "^4.21.1",
        "express-rate-limit": "^7.1.5",
        "express-session": "^1.18.1",
        "google-auth-library": "^9.15.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongodb": "^6.10.0",
        "mongoose": "^8.8.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.16",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hot-toast": "^2.5.2",
        "react-router-dom": "^6.21.3",
        "react-toastify": "^11.0.5",
        "tailwindcss": "^3.4.14",
        "twilio": "^5.5.2",
        "validator": "^13.15.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "babel-loader": "^10.0.0",
        "concurrently": "^8.2.2",
        "css-loader": "^7.1.2",
        "eslint": "^8.57.0",
        "html-webpack-plugin": "^5.6.3",
        "jest": "^29.7.0",
        "morgan": "^1.10.0",
        "nodemon": "^3.0.3",
        "postcss-loader": "^8.1.1",
        "style-loader": "^4.0.0",
        "supertest": "^6.3.4",
        "webpack": "^5.98.0",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
